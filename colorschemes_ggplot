  statsA <-  stat_summary(fun = "median", geom = "bar", fill = "transparent", color = "black", width= 0.4, size = 0.3)
  statsB <-  stat_summary(fun = median, geom = "errorbar", width = 0.2, color = "black", size = 0.4,
             fun.max = function(x) median(x) + sd(x) / sqrt(length(x)),                                 # calculates standard error 
             fun.min = function(x) median(x) - sd(x) / sqrt(length(x)))                                 # calculates standard error 
  
