themeA <- theme_classic()+theme(
    panel.border = element_blank(),
    #panel.grid.major.x = element_blank(), panel.grid.minor.x = element_blank(), 
    panel.grid.major = element_line(size = 0, linetype = 'solid', colour = "grey"),
    panel.grid.minor = element_line(size = 0, linetype = 'solid', colour = "grey"), 
    strip.background=element_rect(colour="white", fill="white"),
    axis.text.x = element_text(size=18, angle = 45, vjust = 0.9, hjust = 1, color = "black"),
    axis.text.y = element_text(             size=16,             vjust = 0.9, hjust = 1, color = "black"),
    axis.title.y = element_text(size=18,  color = "black"),
    axis.title.x = element_text(size=18,  color = "black"),
    plot.title=element_text(hjust=0.5, size=18),
    legend.position = "none")
  
